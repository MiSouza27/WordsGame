#Fase Um

<FaseUm>:
    BoxLayout:
        id: box
        canvas.before:
            Color:
                rgba: 1,1,1,1
            Rectangle:
                size: self.size
                pos: self.pos
                source: 'back_clear.png'
        orientation: 'vertical'
        ActionBar:
            ActionView:
                ActionPrevious:
                    title:'COMPLETE COM X OU CH'
                    with_previous: False
                    on_release: app.root.current ='play'
                ActionButton:
                    text: 'Sair'
                    on_release: app.stop()
    Widget:
    BoxLayout:
        padding: 10
        size_hint_y: None
        height: 60
        spacing: 10
        Botao:
            font_size:22
            bold: True
            text: 'Ver Palavras'
            on_press: app.root.current='fase1c'
        Pontos:
            id: ponto
            text: '0'
            font_size: 22
            bold: True
            height: self.font_size

